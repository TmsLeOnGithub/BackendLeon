<table class="table table-dark">
    <thead>
        <tr class="text-warning">
            <th>Titulo</th>
            <th>Precio</th>
            <th>thumbnail</th>
        </tr>
    </thead>
    <tbody>
        {{#if productos.length}}
        {{#each productos}}
        <tr>
            <td>
                {{this.titulo}}
            </td>
            <td>
                {{this.precio}}
            </td>
            <td>
                {{this.thumbnail}}
            </td>
        </tr>
        {{/each}}
        {{/if}}
    </tbody>
</table>
